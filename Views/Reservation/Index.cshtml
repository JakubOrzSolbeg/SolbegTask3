@model SolbegTask3.Models.Reservation.ReservationRootModel

@{
    ViewBag.Title = "Workplace search";
    Layout = "_Layout";
}

@await Html.PartialAsync("SearchForm", model: Model.SearchParams)
@await Html.PartialAsync("Workplace", model: Model.AwaibleWorkplaces,
    new ViewDataDictionary(ViewData)
        {
            { "TimeFrom", Model.SearchParams.DateFrom!.Value.Date.ToString("yyyy-M-d")},
            { "TimeTo", Model.SearchParams.DateTo!.Value.Date.ToString("yyyy-M-d")}
        })

